<%= simple_form_for(@objective) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :assignment %>
    <%= f.association :mastery_category %>
    <%= f.input :description %>
    <%= f.input :objective_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
